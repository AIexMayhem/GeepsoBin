<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DBF4AD"
    tools:context=".MainActivity">


    <ImageButton
        android:id="@+id/settingsButton"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="292dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/button2_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cog_1"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/addButton"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/button2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/plus_1"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="348dp"
        android:layout_height="491dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="287dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle_2" />

    <ImageView
        android:id="@+id/logoView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="164dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/polygon_1"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="@+id/logoView"
        app:layout_constraintTop_toTopOf="@+id/logoView"
        app:srcCompat="@drawable/svgrepo_iconcarrier"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/mainPanel"
        android:layout_width="342dp"
        android:layout_height="84dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="176dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle_5" />

    <ImageButton
        android:id="@+id/grid_even"
        android:layout_width="86dp"
        android:layout_height="63dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="123dp"
        android:background="@drawable/button3"
        app:layout_constraintEnd_toEndOf="@+id/mainPanel"
        app:layout_constraintTop_toTopOf="@+id/mainPanel"
        app:srcCompat="@drawable/grid_even_1"
        tools:ignore="MissingConstraints" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="322dp"
        android:layout_height="367dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        </LinearLayout>

    </ScrollView>

    <View
        android:id="@+id/sepView"
        android:layout_width="342dp"
        android:layout_height="2dp"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="103dp"
        android:background="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintStart_toStartOf="@+id/imageView9" />

    <ImageButton
        android:id="@+id/align_left"
        android:layout_width="83dp"
        android:layout_height="64dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="17dp"
        android:background="@drawable/button3"
        app:layout_constraintEnd_toEndOf="@+id/mainPanel"
        app:layout_constraintTop_toTopOf="@+id/mainPanel"
        app:srcCompat="@drawable/align_left_1"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/sort_one"
        android:layout_width="91dp"
        android:layout_height="62dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="232dp"
        android:background="@drawable/button3"
        app:layout_constraintEnd_toEndOf="@+id/mainPanel"
        app:layout_constraintTop_toTopOf="@+id/mainPanel"
        app:srcCompat="@drawable/sort_one"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/arrowButton1"
        android:layout_width="306dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/button3"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:srcCompat="@drawable/chevron_down_1"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>